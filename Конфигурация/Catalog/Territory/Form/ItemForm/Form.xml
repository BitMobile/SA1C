		<Attribute name="OutletsSelect" id="3">
			<Title>
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Outlets select</v8:content>
				</v8:item>
			</Title>
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	Outlets.Ref AS Outlet,
	Outlets.Class,
	Outlets.Type,
	TerritoryOutlets.Ref AS Territory,
	TerritoryOutlets.Ref.Owner AS Region,
	Outlets.Distributor AS Distributor
FROM
	Catalog.Outlet AS Outlets
		LEFT JOIN Catalog.Territory.Outlets AS TerritoryOutlets
		ON Outlets.Ref = TerritoryOutlets.Outlet

GROUP BY
	Outlets.Ref,
	Outlets.Class,
	Outlets.Type,
	TerritoryOutlets.Ref,
	TerritoryOutlets.Ref.Owner,
	Outlets.Distributor</QueryText>
