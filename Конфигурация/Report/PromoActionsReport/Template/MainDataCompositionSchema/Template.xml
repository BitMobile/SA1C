////////////////////////////////////////////////////////////////////////////////
SELECT
	BEGINOFPERIOD(AnswersSliceLast.Period, DAY) AS Date,
	ChildQuestions.ChildQuestion,
	AnswersSliceLast.Outlet,
	AnswersSliceLast.SKU,
	AnswersSliceLast.Answer,
	AnswersSliceLast.Visit.SR AS SR
INTO Answers
FROM
	InformationRegister.Answers.SliceLast AS AnswersSliceLast
		LEFT JOIN ChildQuestions AS ChildQuestions
		ON AnswersSliceLast.Question = ChildQuestions.ChildQuestion
WHERE
	NOT ChildQuestions.ChildQuestion IS NULL 
{WHERE
	(AnswersSliceLast.Period &gt;= &amp;StartPeriod),
	(AnswersSliceLast.Period &lt;= &amp;EndPeriod)}
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	Answers.Date,
	Answers.ChildQuestion,
	Answers.Outlet,
	Answers.SKU,
	Answers.Answer,
	MAX(TerritoryOutlets.Ref) AS Region,
	MAX(TerritoryOutlets.Ref.Owner) AS Territory,
	Answers.SR
INTO AnswersTerritory
FROM
	Answers AS Answers
		LEFT JOIN Catalog.Territory.Outlets AS TerritoryOutlets
		ON Answers.Outlet = TerritoryOutlets.Outlet

GROUP BY
	Answers.Date,
	Answers.ChildQuestion,
	Answers.SKU,
	Answers.Outlet,
	Answers.Answer,
	Answers.SR
;

