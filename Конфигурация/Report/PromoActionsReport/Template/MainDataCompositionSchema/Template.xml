		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	BEGINOFPERIOD(Visit.Date, DAY) AS Date,
	Visit.Outlet,
	AllSKUs.Brand,
	AllSKUs.Owner AS SKUGroup,
	VisitSKUs.SKU,
	VisitSKUs.Answer,
	QuestionsInQuestionnairesSliceLast.ChildQuestion,
	TerritoryOutlets.Ref AS Territory,
	TerritoryOutlets.Ref.Owner AS Region,
	Visit.Outlet.Class,
	Visit.Outlet.Type,
	Visit.Outlet.Distributor,
	Visit.SR,
	VisitSKUs.SKU.Brand
FROM
	InformationRegister.QuestionsInQuestionnaires.SliceLast AS QuestionsInQuestionnairesSliceLast
		LEFT JOIN Document.Visit.SKUs AS VisitSKUs
			LEFT JOIN Document.Visit AS Visit
				LEFT JOIN Catalog.Territory.Outlets AS TerritoryOutlets
				ON Visit.Outlet = TerritoryOutlets.Outlet
			ON VisitSKUs.Ref = Visit.Ref
			LEFT JOIN Catalog.SKU AS AllSKUs
			ON VisitSKUs.SKU = AllSKUs.Ref
		ON QuestionsInQuestionnairesSliceLast.ChildQuestion = VisitSKUs.Question</query>
