	AnswersTerritory.SKU,
	AnswersTerritory.Answer,
	AnswersTerritory.Region,
	AnswersTerritory.Territory,
	AnswersTerritory.Outlet.Distributor,
	AnswersTerritory.Outlet.Class,
	AnswersTerritory.Outlet.Type,
	AnswersTerritory.SKU.Brand AS Brand,
	AnswersTerritory.SKU.Owner AS SKUGroup,
	OutletParameters.Value,
	AnswersTerritory.SR
FROM
	AnswersTerritory AS AnswersTerritory
		LEFT JOIN Catalog.Outlet.Parameters AS OutletParameters
		ON AnswersTerritory.Outlet = OutletParameters.Ref
{WHERE
	(OutletParameters.Parameter = &amp;Parameter)}</query>
