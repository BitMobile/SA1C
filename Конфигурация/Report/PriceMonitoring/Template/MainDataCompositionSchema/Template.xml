		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	BEGINOFPERIOD(VisitSKUs.Ref.Date, DAY) AS Date,
	VisitSKUs.Ref.Outlet,
	VisitSKUs.SKU.Brand,
	VisitSKUs.SKU.Owner,
	VisitSKUs.SKU,
	VisitSKUs.Answer,
	MAX(TerritoryOutlets.Ref) AS Territory,
	MAX(TerritoryOutlets.Ref.Owner) AS Region,
	VisitSKUs.Ref.Outlet.Distributor,
	VisitSKUs.Ref.Outlet.Class,
	VisitSKUs.Ref.Outlet.Type,
	VisitSKUs.Ref.SR
INTO VisitTerritory
FROM
	Document.Visit.SKUs AS VisitSKUs
		LEFT JOIN Catalog.Territory.Outlets AS TerritoryOutlets
		ON VisitSKUs.Ref.Outlet = TerritoryOutlets.Outlet
WHERE
	VisitSKUs.Question.Assignment = VALUE(Enum.SKUQuestions.Price)
{WHERE
	(VisitSKUs.Ref.Date &gt;= &amp;StartDate),
	(VisitSKUs.Ref.Date &lt;= &amp;EndDate)}

GROUP BY
	VisitSKUs.Ref.Outlet,
	BEGINOFPERIOD(VisitSKUs.Ref.Date, DAY),
	VisitSKUs.SKU,
	VisitSKUs.Answer,
	VisitSKUs.SKU.Brand,
	VisitSKUs.SKU.Owner,
	VisitSKUs.Ref.Outlet.Distributor,
	VisitSKUs.Ref.Outlet.Class,
	VisitSKUs.Ref.Outlet.Type,
	VisitSKUs.Ref.SR
;

