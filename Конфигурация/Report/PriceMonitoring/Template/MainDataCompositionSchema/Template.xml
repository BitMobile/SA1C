	VisitTerritory.Territory,
	VisitTerritory.Region,
	VisitTerritory.OutletDistributor,
	VisitTerritory.OutletClass,
	VisitTerritory.OutletType,
	VisitTerritory.SR
INTO VisitTerritorySRs
FROM
	VisitTerritory AS VisitTerritory
;

