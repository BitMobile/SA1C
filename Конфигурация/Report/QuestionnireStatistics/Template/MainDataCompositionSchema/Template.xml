		<dataSource>DataSource1</dataSource>
		<query>SELECT
	VisitQuestions.Ref AS VisitRef,
	VisitQuestions.Question,
	VisitQuestions.Answer,
	QuestionnaireQuestions.Ref AS Anketa
INTO Questions
FROM
	Document.Visit.Questions AS VisitQuestions
		LEFT JOIN Document.Questionnaire.Questions AS QuestionnaireQuestions
		ON VisitQuestions.Question = QuestionnaireQuestions.Question
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	Visit.Date,
	Visit.Outlet,
	Visit.SR,
	Visit.Ref
INTO Visits
FROM
	Document.Visit AS Visit
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	OutletList.Ref,
	OutletList.Type,
	OutletList.Class,
	OutletList.Distributor,
	OutletList.Address,
	OutletList.ReceivableLimit,
	TerritoryOutlets.Ref AS Territory,
	Territory.Owner AS Region
INTO Outlets
FROM
	Catalog.Territory.Outlets AS TerritoryOutlets
		LEFT JOIN Catalog.Outlet AS OutletList
		ON TerritoryOutlets.Outlet = OutletList.Ref
		LEFT JOIN Catalog.Territory AS Territory
		ON TerritoryOutlets.Ref = Territory.Ref
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	Visits.Date,
	Visits.Outlet,
	Visits.SR,
	Questions.Question,
	Questions.Answer,
	Questions.Anketa,
	Visits.Ref AS VS
INTO FirstJoin
FROM
	Questions AS Questions
		LEFT JOIN Visits AS Visits
		ON Questions.VisitRef = Visits.Ref
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	FirstJoin.Date,
	FirstJoin.Outlet,
	FirstJoin.SR,
	FirstJoin.Question,
	FirstJoin.Answer,
	Outlets.Ref,
	Outlets.Type,
	Outlets.Class,
	Outlets.Distributor,
	Outlets.Address,
	Outlets.ReceivableLimit,
	FirstJoin.Anketa,
	Outlets.Territory,
	Outlets.Region,
	FirstJoin.VS
FROM
	FirstJoin AS FirstJoin
		LEFT JOIN Outlets AS Outlets
		ON FirstJoin.Outlet = Outlets.Ref
WHERE
	FirstJoin.Date &lt;= &amp;КонецПериода
	AND FirstJoin.Date &gt;= &amp;НачалоПериода</query>
