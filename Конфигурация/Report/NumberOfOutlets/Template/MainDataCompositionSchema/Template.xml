			<field>ТорговыйАгент</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Торговый агент</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоТорговыхТочек</dataPath>
			<field>КоличествоТорговыхТочек</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Количество торговых точек</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>DataSource1</dataSource>
		<query>SELECT
	Territory.Ref AS Территория,
	Territory.SR AS ТорговыйАгент,
	COUNT(DISTINCT TerritoryOutlets.Outlet) AS КоличествоТорговыхТочек,
	TerritoryOutlets.Outlet.OutletStatus AS СтатусТТ,
	Territory.Owner.Manager AS Ответственный,
	Territory.Owner AS Регион,
	TerritoryOutlets.Outlet.Type AS ТипТТ
FROM
	Catalog.Territory.Outlets AS TerritoryOutlets
		INNER JOIN Catalog.Territory AS Territory
		ON TerritoryOutlets.Ref = Territory.Ref
WHERE
	(TerritoryOutlets.Outlet.OutletStatus = VALUE(Перечисление.OutletStatus.Active)
			OR TerritoryOutlets.Outlet.OutletStatus = VALUE(Перечисление.OutletStatus.Potential))

GROUP BY
	Territory.Ref,
	TerritoryOutlets.Outlet.OutletStatus,
	Territory.Owner.Manager,
	Territory.SR,
	TerritoryOutlets.Outlet.Type,
	Territory.Owner</query>
