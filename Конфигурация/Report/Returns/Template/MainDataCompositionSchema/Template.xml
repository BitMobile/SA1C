		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	ReturnSKUs.Ref.SR,
	ReturnSKUs.Ref.Outlet,
	ReturnSKUs.Ref AS Return,
	ReturnSKUs.SKU,
	ReturnSKUs.Price,
	ReturnSKUs.Discount,
	ReturnSKUs.Amount,
	ReturnSKUs.Qty,
	ReturnSKUs.Units,
	ReturnSKUs.Ref.PriceList.Description,
	ReturnSKUs.Ref.Commentary,
	ReturnSKUs.Ref.Outlet.Type,
	ReturnSKUs.Ref.Outlet.Class,
	NestedSelect.Territory,
	NestedSelect.Region
FROM
	Document.Return.SKUs AS ReturnSKUs
		LEFT JOIN (SELECT
			MAX(TerritoryOutlets.Ref.Owner) AS Region,
			MAX(TerritoryOutlets.Ref) AS Territory,
			TerritoryOutlets.Outlet AS Outlet
		FROM
			Catalog.Territory.Outlets AS TerritoryOutlets
		
		GROUP BY
			TerritoryOutlets.Outlet) AS NestedSelect
		ON ReturnSKUs.Ref.Outlet = NestedSelect.Outlet
{WHERE
	(ReturnSKUs.Ref.Date &gt;= &amp;DateStart),
	(ReturnSKUs.Ref.Date &lt;= &amp;DateEnd)}</query>
