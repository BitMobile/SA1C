		<query>SELECT ALLOWED
	TerritoryOutlets.Ref.Owner AS Region,
	TerritoryOutlets.Ref AS Territory,
	Answers.Outlet.Distributor,
	Answers.Outlet.Class,
	Answers.Outlet.Type,
	Answers.Outlet,
	Answers.SKU.Brand,
	Answers.SKU.Owner AS SKUGroup,
	Answers.SKU,
	AVG(CAST(Answers.Answer AS NUMBER)) AS AvgStock
INTO Average
FROM
	InformationRegister.Answers AS Answers
		LEFT JOIN Catalog.Territory.Outlets AS TerritoryOutlets
		ON Answers.Outlet = TerritoryOutlets.Outlet
{WHERE
	(Answers.Period &lt;= &amp;Period),
	(Answers.Period &gt;= DATEADD(&amp;Period, MONTH, -2))}

GROUP BY
	Answers.Outlet,
	Answers.SKU,
	TerritoryOutlets.Ref,
	TerritoryOutlets.Ref.Owner,
	Answers.Outlet.Distributor,
	Answers.Outlet.Class,
	Answers.Outlet.Type,
	Answers.SKU.Brand,
	Answers.SKU.Owner
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	TerritoryOutlets.Ref.Owner AS Region,
	TerritoryOutlets.Ref AS Territory,
	AnswersSliceLast.Outlet.Distributor,
	AnswersSliceLast.Outlet.Class,
	AnswersSliceLast.Outlet.Type,
	AnswersSliceLast.Outlet,
	AnswersSliceLast.SKU.Brand,
	AnswersSliceLast.SKU.Owner AS SKUGroup,
	AnswersSliceLast.SKU,
	CAST(AnswersSliceLast.Answer AS NUMBER) AS Stock
INTO AllBrands
FROM
	InformationRegister.Answers.SliceLast(, QuestionAssignment = VALUE(Enum.SKUQuestions.Stock)) AS AnswersSliceLast
		LEFT JOIN Catalog.Territory.Outlets AS TerritoryOutlets
		ON AnswersSliceLast.Outlet = TerritoryOutlets.Outlet
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	TerritoryOutlets.Ref.Owner AS Region,
	TerritoryOutlets.Ref AS Territory,
	AnswersSliceLast.Outlet.Distributor,
	AnswersSliceLast.Outlet.Class,
	AnswersSliceLast.Outlet.Type,
	AnswersSliceLast.Outlet,
	AnswersSliceLast.SKU.Brand,
	AnswersSliceLast.SKU.Owner AS SKUGroup,
	AnswersSliceLast.SKU,
	CAST(AnswersSliceLast.Answer AS NUMBER) AS CurrentBrandStock
INTO CurrentBrand
FROM
	InformationRegister.Answers.SliceLast(, QuestionAssignment = VALUE(Enum.SKUQuestions.Stock)) AS AnswersSliceLast
		LEFT JOIN Catalog.Territory.Outlets AS TerritoryOutlets
		ON AnswersSliceLast.Outlet = TerritoryOutlets.Outlet
WHERE
	AnswersSliceLast.SKU.Brand = &amp;Brand
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	TerritoryOutlets.Ref.Owner AS Region,
	TerritoryOutlets.Ref AS Territory,
	AnswersSliceLast.Outlet.Distributor,
	AnswersSliceLast.Outlet.Class,
	AnswersSliceLast.Outlet.Type,
	AnswersSliceLast.Outlet,
	AnswersSliceLast.SKU.Owner AS SKUGroup,
	SUM(CAST(AnswersSliceLast.Answer AS NUMBER)) AS SKUGroupStock
INTO SKUGroups
FROM
	InformationRegister.Answers.SliceLast(, QuestionAssignment = VALUE(Enum.SKUQuestions.Stock)) AS AnswersSliceLast
		LEFT JOIN Catalog.Territory.Outlets AS TerritoryOutlets
		ON AnswersSliceLast.Outlet = TerritoryOutlets.Outlet

GROUP BY
	AnswersSliceLast.SKU.Owner,
	AnswersSliceLast.Outlet,
	TerritoryOutlets.Ref,
	TerritoryOutlets.Ref.Owner,
	AnswersSliceLast.Outlet.Distributor,
	AnswersSliceLast.Outlet.Class,
	AnswersSliceLast.Outlet.Type
;

////////////////////////////////////////////////////////////////////////////////
SELECT ALLOWED
	AllBrands.Region,
	AllBrands.Territory,
	AllBrands.OutletDistributor,
	AllBrands.OutletClass,
	AllBrands.OutletType,
	AllBrands.Outlet,
	AllBrands.SKUBrand,
	AllBrands.SKUGroup,
	AllBrands.SKU,
	AllBrands.Stock,
	CurrentBrand.CurrentBrandStock,
	SKUGroups.SKUGroupStock,
	0 AS Percent,
	Average.AvgStock,
	OutletParameters.Value
FROM
	AllBrands AS AllBrands
		LEFT JOIN CurrentBrand AS CurrentBrand
		ON AllBrands.Region = CurrentBrand.Region
			AND AllBrands.Territory = CurrentBrand.Territory
			AND AllBrands.OutletDistributor = CurrentBrand.OutletDistributor
			AND AllBrands.OutletClass = CurrentBrand.OutletClass
			AND AllBrands.OutletType = CurrentBrand.OutletType
			AND AllBrands.Outlet = CurrentBrand.Outlet
			AND AllBrands.SKUBrand = CurrentBrand.SKUBrand
			AND AllBrands.SKUGroup = CurrentBrand.SKUGroup
			AND AllBrands.SKU = CurrentBrand.SKU
		LEFT JOIN SKUGroups AS SKUGroups
		ON AllBrands.Region = SKUGroups.Region
			AND AllBrands.Territory = SKUGroups.Territory
			AND AllBrands.OutletDistributor = SKUGroups.OutletDistributor
			AND AllBrands.OutletClass = SKUGroups.OutletClass
			AND AllBrands.OutletType = SKUGroups.OutletType
			AND AllBrands.Outlet = SKUGroups.Outlet
			AND AllBrands.SKUGroup = SKUGroups.SKUGroup
		LEFT JOIN Average AS Average
		ON AllBrands.Region = Average.Region
			AND AllBrands.Territory = Average.Territory
			AND AllBrands.OutletDistributor = Average.OutletDistributor
			AND AllBrands.OutletClass = Average.OutletClass
			AND AllBrands.OutletType = Average.OutletType
			AND AllBrands.Outlet = Average.Outlet
			AND AllBrands.SKUBrand = Average.SKUBrand
			AND AllBrands.SKUGroup = Average.SKUGroup
			AND AllBrands.SKU = Average.SKU
		LEFT JOIN Catalog.Outlet.Parameters AS OutletParameters
		ON AllBrands.Outlet = OutletParameters.Ref
WHERE
	OutletParameters.Parameter = &amp;Parameter</query>
