					<v8:content>Ответственный</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	COUNT(DISTINCT VisitSKUs.SKU) AS Номенклатура,
	VisitSKUs.Ref AS Визит,
	VisitSKUs.Ref.Date AS ДатаВизита,
	VisitSKUs.Ref.SR AS ТорговыйАгент,
	VisitSKUs.Ref.Outlet AS ТорговаяТочка,
	VisitSKUs.Ref.Outlet.OutletStatus AS Статус,
	TerritoryOutlets.Ref.Owner AS Регион,
	TerritoryOutlets.Ref AS Территория,
	TerritoryOutlets.Ref.Owner.Manager AS Ответственный
FROM
	Document.Visit.SKUs AS VisitSKUs
		INNER JOIN Catalog.Territory.Outlets AS TerritoryOutlets
		ON VisitSKUs.Ref.Outlet = TerritoryOutlets.Outlet
WHERE
	VisitSKUs.Ref.Date &gt;= &amp;НачалоПериода
	AND VisitSKUs.Ref.Date &lt;= &amp;КонецПериода
	AND VisitSKUs.Question.Assignment = VALUE(Enum.SKUQuestions.Available)
	AND (VisitSKUs.Answer = "Yes"
			OR VisitSKUs.Answer = "Да")

GROUP BY
	VisitSKUs.Ref,
	VisitSKUs.Ref.Date,
	VisitSKUs.Ref.SR,
	VisitSKUs.Ref.Outlet,
	VisitSKUs.Ref.Outlet.OutletStatus,
	TerritoryOutlets.Ref.Owner,
	TerritoryOutlets.Ref,
	TerritoryOutlets.Ref.Owner.Manager</query>
