			<field>Qty</field>
		</field>
		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	OrderSKUs.Ref.SR,
	OrderSKUs.Ref.Outlet,
	OrderSKUs.Ref AS Order,
	OrderSKUs.SKU,
	OrderSKUs.Price,
	OrderSKUs.Discount,
	OrderSKUs.Amount,
	OrderSKUs.Qty,
	OrderSKUs.Units
FROM
	Document.Order.SKUs AS OrderSKUs
WHERE
	OrderSKUs.Ref.Date BETWEEN &amp;DateStart AND &amp;DateEnd</query>
